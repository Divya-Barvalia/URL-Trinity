# WhoisXML – https://whoisxmlapi.com/ (WHOIS API)
WHOISXML_API_KEY=

# Google Safe Browsing – https://console.cloud.google.com/apis/credentials
GOOGLE_SAFE_BROWSING_API_KEY=
